{
  "site": {
    "title": "Limit's Blog",
    "base_url": "https://astroplate-multilang.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo.png",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "150",
    "logo_height": "30",
    "logo_text": "Limit's Blog"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 200,
    "blog_folder": "blog",
    "default_language": "en",
    "disable_languages": [],
    "default_language_in_subdir": false
  },

  "announcement": {
    "enable": false,
    "expire_days": 7
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "Â© 2026 Limit | Built with Astro"
  },

  "navigation_button": {
    "enable": false,
    "label": "Github",
    "link": "https://github.com/zeon-studio/astroplate/tree/multilingual"
  },

  "google_tag_manager": {
    "enable": false,
    "gtm_id": "GTM-XXXXXX"
  },

  "disqus": {
    "enable": false,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "Limit",
    "meta_image": "/images/og-image.png",
    "meta_description": "Exploring the world of Digital Signal Processing, Computer Vision, and Robotics."
  }
}
